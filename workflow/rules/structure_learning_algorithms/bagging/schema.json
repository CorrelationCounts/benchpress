{
    "description": "Bagging algorithm",
    "title": "bagging",
    "type": "array",
    "items": {
        "title": "bagging",
        "description": "Bagging algorithm",
        "type": "object",
        "properties": {
            "id": {
                "type": "string",
                "description": "Unique identifier",
                "enum":["bagging"]
            },
            "category": {
                "type": "string",
                "description": "Type of bagging (standard or weighted)",
                "enum": ["standard","weighted"]
            },"ids":{
                "type": "string",
                "description": "csv string of ids, if weighted bagging is used, id should be followed by its weight"
            },
            "bagging_threshold":{
                "oneOf":[
                {"$ref": "../../../schemas/definitions.schema.json#/definitions/nonzeroprob"},
                {"type": "array", "items": {"$ref": "../../../schemas/definitions.schema.json#/definitions/nonzeroprob"}, "minItems": 1}
            ]
            },
            "timeout": {
                "$ref": "../../../schemas/definitions.schema.json#/definitions/flexnonnegnumnull"
            },
            "edgeConstraints": {
                "type": ["string", "null"],
                "pattern": ".*\\.json$",
                "description": "Name of the JSON file containing background knowledge"
            }
        },
        "additionalProperties": false,
        "required": ["id","category","ids"],
        "examples": [
            {
                "id": "bagging",
                "category": "standard",
                "ids": "id1:id2:id3"
            },
            {
                "id": "bagging",
                "category": "standard",
                "bagging_threshold": 0.6,
                "ids": "id1:id2:id3"
            },{
                "id": "bagging",
                "category": "weighted",
                "ids": "id1:0.5:id2:0.25:id3:0.25"
            },{
                "id": "bagging",
                "category": "weighted",
                "bagging_threshold": 0.55,
                "ids": "id1:0.5:id2:0.25:id3:0.25"
            }
        ]
    }
}