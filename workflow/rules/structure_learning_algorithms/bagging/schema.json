{
    "description": "Bagging algorithm",
    "title": "bagging",
    "type": "array",
    "items": {
        "title": "bagging",
        "description": "Bagging algorithm",
        "type": "object",
        "properties": {
            "id": {
                "type": "string",
                "description": "Unique identifier"
            },
            "category": {
                "type": "string",
                "description": "Type of bagging (standard or weighted)",
                "enum": ["standard","weighted"]
            },"ids":{
                "type": "array",
                "items":{
                    "type": "string"
                }
            },
            "threshold":{
                "$ref": "../../../schemas/definitions.schema.json#/definitions/nonzeroprob"
            },
            "weights":{
                "type": "object",
                "minProperties": 1,
                "patternProperties": {
                    "^[^\\s]+$": {"$ref": "../../../schemas/definitions.schema.json#/definitions/nonnegnum"}
                },
                "additionalProperties": false
            },
            "edgeConstraints": {
                "type": ["string", "null"],
                "pattern": ".*\\.json$",
                "description": "Name of the JSON file containing background knowledge"
            }
        },
        "additionalProperties": false,
        "required": ["category","ids"],
        "examples": [
            {
                "id": "bagging",
                "category": "standard",
                "ids": ["id1","id2","id3"],
                "edgeConstraints": "edgeConstraints.json"
            },
            {
                "id": "bagging",
                "category": "weighted",
                "threshold": 0.5,
                "ids": ["id1","id2","id3"],
                "weights": {"id1":0.5,"id2":0.25,"id3":0.25},
                "edgeConstraints": "edgeConstraints.json"
            }
        ]
    }
}